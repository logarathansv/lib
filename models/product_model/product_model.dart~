class Product {
  String? id;
  String name;
  String? description;
  String price;
  String quantity;
  String? imageUrl;

  Product({
    this.id,
    required this.name,
    this.description,
    required this.price,
    required this.quantity,
    this.imageUrl,
  });
  factory Product.fromJson(Map<String,dynamic> json){
    return Product(
        id: json['PId'] as String,
        name:json['Pname'] as String,
        description: json['Pdesc'] as String,
        price: json['Pprice'] as String,
        quantity: json['Pqty'] as String,
        imageUrl: json['PimageUrl'] as String
    );
  }

}